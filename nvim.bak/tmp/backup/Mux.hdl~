/** 
 * Multiplexor:
 * out = ((sel == 0), a, b)
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Replace this comment with your code.
    Not (in=sel, out=nsel);
    And (a=a, b=nsel, out=ca);
    And (a=b, b=sel, out=cb);
    Or (a=ca, b=cb, out=out);
}
