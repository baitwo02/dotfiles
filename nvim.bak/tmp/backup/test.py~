mid_members = int(len(scores_chushi) / 2 )
scores_chushi.sort(reverse=True)
scores_fushi.sort(reverse=True)
scores_final.sort(reverse=True)

print("\n初试成绩:")
for i in range(len(scores_chushi)):
    print(i, ":", scores_chushi[i])

print("\n==========")

print("\n复试成绩:")
for i in range(len(scores_fushi)):
    print(i, ":", scores_fushi[i])

print("\n==========")

print("\n总成绩:")
for i in range(len(scores_final)):
    print(i, ":", scores_final[i])

print("\n==========")

print("\n初始分数中位名次:", scores_chushi[mid_members])
for i in range(len(scores_chushi)):
    if scores_chushi[i] == 370:
        print("初始分数为370名次", i)
        break

print("\n复试分数中位名次:", scores_fushi[mid_members])
for i in range(len(scores_fushi)):
    if scores_fushi[i] == 80:
        print("复试分数为80名次", i)
        break

print("\n总成绩中位名次:", scores_final[mid_members])
for i in range(len(scores_final)):
    if scores_final[i] == 79:
        print("总成绩分数为79(初始368+复试84)名次:", i)
        break

print("初始220名分数", scores_chushi[220])
print("复试220名分数", scores_fushi[220])
